<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="
           http://www.liquibase.org/xml/ns/dbchangelog
           https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">

    <changeSet id="5" author="kuza">
    <preConditions onFail="MARK_RAN">
        <not>
            <tableExists tableName="user_entity_types"/>
        </not>
    </preConditions>

   <createTable tableName="user_entity_types">
    <column name="user_id" type="BIGINT UNSIGNED"/>
    <column name="entity_type" type="VARCHAR(255)"/>
  </createTable>

        <addForeignKeyConstraint
                baseTableName="user_entity_types"
                baseColumnNames="user_id"
                referencedTableName="users"
                referencedColumnNames="id"
                constraintName="fk_user_entity_types_user"/>
    </changeSet>
</databaseChangeLog>
